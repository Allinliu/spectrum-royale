<view:DemoPane xmlns:fx="http://ns.adobe.com/mxml/2009"
  xmlns:view="view.*" xmlns:sp="library://ns.unhurdle.com/spectrum"
  xmlns:html="library://ns.apache.org/royale/html"
  title="Asset">
  <fx:Script>
    <![CDATA[
    ]]>
  </fx:Script>
  
  <sp:Heading size="4" text="Image"/>
  <html:Div height="12"/>
  <sp:ImageAsset src="https://opensource.adobe.com/spectrum-css/static/images/example-ava.jpg" width="128" height="128"/>
  <html:Div height="12"/>
  <sp:TypographyGroup>
    <sp:Heading size="5" text="Markup"/>
    <html:Div height="12"/>
    <sp:Code text='&lt;sp:ImageAsset src="https://opensource.adobe.com/spectrum-css/static/images/example-ava.jpg" width="128" height="128"/&gt;' size="4" style="white-space:break-spaces;"/>
  </sp:TypographyGroup>
  <html:Div height="12"/>

  <sp:Heading size="4" text="File"/>
  <html:Div height="12"/>
  <sp:FileAsset width="128" height="128"/>
  <html:Div height="12"/>
  <sp:TypographyGroup>
    <sp:Heading size="5" text="Markup"/>
    <html:Div height="12"/>
    <sp:Code text='&lt;sp:FileAsset width="128" height="128" /&gt;' size="4" style="white-space:break-spaces;"/>
  </sp:TypographyGroup>
  <html:Div height="12"/>

  <sp:Heading size="4" text="Folder"/>
  <html:Div height="12"/>
  <sp:FolderAsset width="128" height="128"/>
  <html:Div height="12"/>
  <sp:TypographyGroup>
    <sp:Heading size="5" text="Markup"/>
    <html:Div height="12"/>
    <sp:Code text='&lt;sp:FolderAsset width="128" height="128" /&gt;' size="4" style="white-space:break-spaces;"/>
  </sp:TypographyGroup>
  <html:Div height="12"/>
</view:DemoPane>